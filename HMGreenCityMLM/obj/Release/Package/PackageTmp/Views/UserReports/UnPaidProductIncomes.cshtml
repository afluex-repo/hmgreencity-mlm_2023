@model HMGreenCityMLM.Models.Reports

@{
    ViewBag.Title = "UnPaidProductIncomes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <br />
        @*<h4 class="page-header">Topup Report</h4>*@

    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Product Unpaid Incomes
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                 <table id="example23" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>Sr No.</th>
                            <th>Date</th>
                            <th>From LoginId</th>
                            <th>From Name</th>
                            <th>Amount</th>

                            <th>Income Type</th>

                            

                        </tr>
                    </thead>
                    <tbody>
                        @if (Model != null)
                        {
                            if (Model.lstunpaidincomes != null)
                            {
                                int i = 1;
                                foreach (var Item in Model.lstunpaidincomes)
                                {
                            <tr class="text-left">
                                <td>@i</td>
                                <td>@Item.Date</td>
                                <td>@Item.FromLoginId</td>
                                <td>@Item.FromUserName</td>

                                <td>@Item.Amount</td>

                                <td>@Item.IncomeType</td>
                               
                            </tr>
                                    i++;
                                }

                            }
                        }
                    </tbody>
                </table>

            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>

<script src="../vendor/jquery/jquery.min.js"></script>

<script>
    $(document).ready(function () {
        $('#dataTables-example').DataTable({
            responsive: true
        });
    });
</script>




