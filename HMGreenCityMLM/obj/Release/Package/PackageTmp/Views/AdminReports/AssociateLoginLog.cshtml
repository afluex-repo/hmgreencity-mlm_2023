@model HMGreenCityMLM.Models.Reports

@{
    ViewBag.Title = "AssociateLoginLog";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@using (Html.BeginForm("AssociateLoginLog", "AdminReports", FormMethod.Post, new { @id = "form1", enctype = "multipart/form-data" }))
{
    <div class="row">
        <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
            <div class="card">
                <div class="card-body">
                    <div class="d-flex align-items-center justify-content-between">
                        <h5 class="mb-0">Associate Login Log</h5>
                        @*<button class="btn btn-info"><a href="#!" class="text-white">Add New News</a></button>*@
                    </div>
                    @*<p style="color: red;display: block; margin-bottom:0px; text-align:center">
                            @TempData["ChangeAsspassword"]
                        </p>*@
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-12 col-md-12 col-lg-12">
            <div class="card">
                <div class="card-body">
                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label>Login Id</label>
                            @Html.TextBoxFor(m => m.LoginId, new { @class = "form-control" })
                        </div>
                        <div class="form-group col-md-3">
                            <label>Name</label>
                            @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
                        </div>
                      
                        <div class="form-group col-md-4">
                            <input class="btn btn-success" type="submit" value="Search" name="Search" id="btnSearch" />
                        </div>
                    </div>
                    <br />
                    <div class="table-responsive">
                        <table class="table table-striped table-hover" id="tableExport" style="width:100%;">
                            <thead>
                                <tr>
                                    <th>Sr No.</th>
                                    <th>Name</th>
                                    <th>Login Id</th>
                                    <th>User Type</th>
                                    <th>Login Date Time</th>
                                    <th>IP</th>
                                    <th>Remarks</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model != null)
                                {
                                    if (Model.lstAssociateLoginLog != null)
                                    {
                                        int i = 1;
                                        foreach (var Item in Model.lstAssociateLoginLog)
                                        {
                                            <tr class="text-left">
                                                <td>@i</td>
                                                <td>@Item.DisplayName</td>
                                                <td>@Item.LoginId</td>
                                                <td>@Item.UserType</td>
                                                <td>@Item.LoginDateTime</td>
                                                <td>@Item.IP</td>
                                                <td>@Item.Remarks</td>
                                            </tr>
                                            i++;
                                        }

                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
}
<style>
    .errortext {
        border: 2px solid red !important;
    }
</style>


<script src="../../WebsiteCss/js/jquery.js"></script>
<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.0/themes/smoothness/jquery-ui.css">

<script>

    $('#FromDate').datepicker({
        dateFormat: 'dd/mm/yy',
        changeMonth: true,
        changeYear: true
    });
    $('#ToDate').datepicker({
        dateFormat: 'dd/mm/yy',
        changeMonth: true,
        changeYear: true
    });
</script>
<script>
    $(document).ready(function () {
        $('#dataTables-example').DataTable({
            responsive: true
        });
    });
    $('#printingdate').datepicker({
        dateFormat: 'dd/mm/yy',
        changeYear: true,
        changeMonth: true,

    });
    function changeprintdate(PrintingDate, LoginId) {
        $('#printingdate').val(PrintingDate);
        $('#hdfloginnid').val(LoginId);
        $('#myModal').show();
    }
    function closepopup() {
        $('#myModal').hide();
    }
    function printrecipt() {
        $(".errortext").removeClass("errortext");
        if ($('#printingdate').val() == '') {
            $("#printingdate").addClass('errortext');
            $('#printingdate').focus();
            return false;
        }
        var LoginId = $('#hdfloginnid').val();
        $.ajax({
            url: '/AdminReports/PrintReciept1',
            type: "GET",
            dataType: "JSON",
            data: { id: LoginId, PrintingDate: $('#printingdate').val() },
            success: function (data) {
                debugger;
                window.location.href = '@Url.Action("PrintReciept", "AdminReports")';
            }
        });
    }
</script>
<style>
    .btn-success {
        margin-top: 29px;
        margin-left: 20px;
    }
</style>




