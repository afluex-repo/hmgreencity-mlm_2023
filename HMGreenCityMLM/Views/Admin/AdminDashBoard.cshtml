@model HMGreenCityMLM.Models.DashBoard
@{
    ViewBag.Title = "AdminDashBoard";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="app-content content">
    <div class="content-overlay"></div>
    <div class="header-navbar-shadow"></div>
    <div class="content-wrapper">
        <div class="content-body">
            <section id="dashboard-analytics">
                <div class="form-group">
                    <p style="color:red;"> @TempData["DistributePaymentPassword"] </p>
                </div>
                <div class="row">
                    <div class="col-lg-3 col-md-6 col-12">
                        <div class="card">
                            <div class="card-header d-flex flex-column align-items-start pb-0">
                                <div class="avatar bg-rgba-primary p-50 m-0">
                                    <div class="avatar-content">
                                        <i class="feather icon-users text-primary font-medium-5"></i>
                                    </div>
                                </div>
                                <h2 class="text-bold-700 mt-1 mb-25">@ViewBag.TotalUsers</h2>
                                <a class="mb-0" style="color:#000000" href="/AdminProfile/AssociateList">Total Associates</a>
                                <br />
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6 col-12">
                        <div class="card">
                            <div class="card-header d-flex flex-column align-items-start pb-0">
                                <div class="avatar bg-rgba-warning p-50 m-0">
                                    <div class="avatar-content">
                                        <i class="feather icon-package text-warning font-medium-5"></i>
                                    </div>
                                </div>
                                <h2 class="text-bold-700 mt-1 mb-25">@ViewBag.ActiveUsers</h2>
                                <a class="mb-0" style="color:#000000" href="/AdminProfile/AssociateList?Status=P">Active Users</a>
                                <br />
                            </div>

                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 col-12">
                        <div class="card">
                            <div class="card-header d-flex flex-column align-items-start pb-0">
                                <div class="avatar bg-rgba-primary p-50 m-0">
                                    <div class="avatar-content">
                                        <i class="feather icon-users text-primary font-medium-5"></i>
                                    </div>
                                </div>
                                <h2 class="text-bold-700 mt-1 mb-25">@ViewBag.InactiveUsers</h2>
                                <a class="mb-0" style="color:#000000" href="/AdminProfile/AssociateList?Status=T">InActive Users</a>

                                <br />
                            </div>

                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 col-12">
                        <div class="card">
                            <div class="card-header d-flex flex-column align-items-start pb-0">
                                <div class="avatar bg-rgba-primary p-50 m-0">
                                    <div class="avatar-content">
                                        <i class="feather icon-users text-primary font-medium-5"></i>
                                    </div>
                                </div>
                                <h2 class="text-bold-700 mt-1 mb-25">@ViewBag.BlockedUsers</h2>
                                <a class="mb-0" style="color:#000000" href="/AdminProfile/AssociateList?Status=B">Blocked Users</a>
                                <br />
                            </div>

                        </div>
                    </div>
                </div>


                <div class="row">
                    <div class="col-lg-3">
                        <div class="card panel-default">
                            <div class="card-header h4">
                                Associate Details
                            </div>
                            <!-- /.panel-heading -->
                            <div class="card-body">
                                <div id="divusers"></div>
                            </div>
                            <!-- /.panel-body -->
                        </div>
                        <!-- /.panel -->
                        <!-- /.panel -->
                    </div>

                    <div class="col-lg-3">
                        <div class="card panel-default">
                            <div class="card-header h4">
                                Payout Status
                            </div>
                            <!-- /.panel-heading -->
                            <div class="card-body">
                                <div id="divpayout"></div>
                            </div>
                            <!-- /.panel-body -->
                        </div>
                        <!-- /.panel -->
                        <!-- /.panel -->
                    </div>

                    <div class="col-lg-6">
                        <div class="card panel-default">
                            <div class="card-header h4">
                                Investment Details
                            </div>
                            <!-- /.panel-heading -->
                            <div class="card-body">
                                <div id="divinvestment"></div>
                            </div>
                            <!-- /.panel-body -->
                        </div>
                        <!-- /.panel -->
                        <!-- /.panel -->
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <div class="card panel-default">
                            <div class="card-header h4">
                                Joining Details
                            </div>
                            <!-- /.panel-heading -->
                            <div class="card-body">
                                <div id="divjoining"></div>
                            </div>
                            <!-- /.panel-body -->
                        </div>
                        <!-- /.panel -->
                        <!-- /.panel -->
                    </div>
                </div>
            </section>
        </div>
    </div>
</div>









