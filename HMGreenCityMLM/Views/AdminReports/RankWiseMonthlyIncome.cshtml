@model HMGreenCityMLM.Models.Reports

@{
    ViewBag.Title = "RankWiseMonthlyIncome";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@using (Html.BeginForm("RankWiseMonthlyIncome", "AdminReports", FormMethod.Post, new { @id = "form1" }))
{
    <div class="row">
        <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
            <div class="card">
                <div class="card-body">
                    <div class="d-flex align-items-center justify-content-between">
                        <h5 class="mb-0">RankWise Monthly Income Reports</h5>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-12 col-md-12 col-lg-12">
            <div class="card">
                <div class="card-body">
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label>Login Id</label>
                            @Html.TextBoxFor(m => m.ToLoginID, new { @class = "form-control", @placeholder = "LoginID" })
                            @Html.HiddenFor(m => m.LoginId)
                        </div>

                        @*<div class="form-group col-md-4">
                            <label>From Date</label>
                            <input type="datetime-local" class="form-control" name="FromDate" id="FromDate" />
                            
                        </div>
                        <div class="form-group col-md-4">
                            <label>To Date</label>
                            <input type="datetime-local" class="form-control" name="ToDate" id="ToDate" />
                            
                        </div>*@
                        @*<div class="form-group col-md-4">
                            <label>Rank</label>
                            @Html.DropDownListFor(m => m.RankIncome, new SelectList(ViewBag.ddlRankIncome, "Value", "Text"), new { @class = "form-control" })
                        </div>*@
                        <div class="form-group col-md-4">
                            <label class="d-block">&nbsp;</label>
                            <input type="submit" id="btnSearch" class="btn btn-success pull-left" name="Search" value="Search" onclick="return validateLogin();" />
                        </div>
                    </div>
                    <br />
                    <div class="table-responsive">
                        <table class="table table-striped table-hover" id="tableExport" style="width:100%;">
                            <thead>
                                <tr>
                                    <th>Sr No.</th>
                                    <th>Login Id</th>
                                    <th>Name</th>
                                    <th>MonthYear</th>
                                    <th>Income</th>
                                    @*<th>Balance</th>*@
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model != null)
                                {
                                    if (Model.lstRankwaisehourlyincomeReports != null)
                                    {
                                        int i = 1;
                                        foreach (var Item in Model.lstRankwaisehourlyincomeReports)
                                        {
                                            <tr class="text-left">
                                                <td>@i</td>
                                                <td>@Item.LoginId</td>
                                                <td>@Item.Name</td>
                                                <td>@Item.FromDate</td>
                                                <td>@Item.Amount</td>
                                                @*<td>@Item.Total</td>*@
                                            </tr>
                                            i++;
                                        }
                                    }
                                }
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th></th>
                                    <th></th>
                                    <th></th>
                                    <th>Total :-</th>
                                    <th>@ViewBag.Total</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

}
<script src="../vendor/jquery/jquery.min.js"></script>
<script src="../../WebsiteCss/js/jquery.js"></script>
<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.0/themes/smoothness/jquery-ui.css">
<script>

    $(document).ready(function () {
        debugger;
        $('#dataTables-example').DataTable({
            responsive: true
        });
        $("input").attr("autocomplete", "off");
        var cityuse = [];
        $("#loader").css({ 'display': '' });
        $.ajax({
            url: '/AdminReports/GetUserList',
            type: 'Post',
            success: function (response) {
                debugger;

                if (response != null) {
                    $(response).each(function (index, item) {
                        cityuse.push(item.UserName + "(" + item.LoginIDD + ")");
                    });

                    $('#ToLoginID').autocomplete({
                        source: cityuse,
                        select: function (event, ui) {
                            var str = ui.item.label;
                            var strsplit = str.split("(");
                            $('#LoginId').val(strsplit[1].substr(0, strsplit[1].length - 1));
                        }
                    })
                }

            }, error: function () {
                alert('Some Thing wrong!');
                $("#loader").css({ 'display': 'none' });
            }
        });
    });
    //$('#FromDate').datepicker({
    //    dateFormat: 'dd/mm/yy',
    //    changeYear: true,
    //    changeMonth: true
    //});
    //$('#ToDate').datepicker({
    //    dateFormat: 'dd/mm/yy',
    //    changeYear: true,
    //    changeMonth: true
    //});


</script>


