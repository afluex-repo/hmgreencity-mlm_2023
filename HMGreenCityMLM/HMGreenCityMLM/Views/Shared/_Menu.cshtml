@model HMGreenCityMLM.Models.Home

<div class="navbar-content">
    <!-- start: SIDEBAR -->
    <div class="main-navigation navbar-collapse collapse">


        <!-- start: MAIN NAVIGATION MENU -->
        <ul class="main-navigation-menu">
            <li>
                <!--active open-->
                <a href="/Admin/AdminDashBoard">
                    <i class="clip-home-3"></i>
                    <span class="title"> Dashboard </span><span class="selected"></span>
                </a>
            </li>
            @if (Model != null)
            {
                if (Model.lstMenu != null)
                {


                    foreach (var item in Model.lstMenu)
                    {
                        <li>
                            <a href="javascript:void(0)">
                                <i class="@item.Icon"></i>
                                <span class="title"> @item.MenuName </span><i class="icon-arrow"></i>
                                <span class="selected"></span>
                            </a>
                            @if (item.Url == "#")
                            {
                                <ul class="sub-menu">
                                    @foreach (var item1 in Model.lstsubmenu)
                                    {
                                        if (item1.MenuId == item.MenuId)
                                        {
                                            <li>
                                                <a href="@item1.Url">
                                                    <span class="title"> @item1.SubMenuName </span>

                                                </a>
                                            </li>
                                        }
                                    }


                                </ul>
                            }
                        </li>
                    }
                }
            }


        </ul>
        <!-- end: MAIN NAVIGATION MENU -->
    </div>
    <!-- end: SIDEBAR -->
</div>