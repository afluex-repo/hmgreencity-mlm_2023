@model HMGreenCityMLM.Models.Reports

@{
    ViewBag.Title = "AssociateLoginLog";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@using (Html.BeginForm("AssociateLoginLog", "AdminReports", FormMethod.Post, new { @id = "form1", enctype = "multipart/form-data" }))
{
    <br />
    <div class="app-content content">
        <div class="content-overlay"></div>
        <div class="header-navbar-shadow"></div>
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body">
                <!-- users edit start -->
                <section class="users-edit">
                    <div class="card">
                        <div class="card-content">
                            <div class="card-body">
                                <ul class="nav nav-tabs mb-3" role="tablist">
                                    <li class="nav-item">
                                        <a class="nav-link d-flex align-items-center active" id="account-tab" data-toggle="tab" href="#account"
                                           aria-controls="account" role="tab" aria-selected="true">
                                            <i class="feather icon-user mr-25"></i><span class="d-none d-sm-block">  Associate Login Log </span>
                                        </a>
                                    </li>

                                </ul>
                                <div class="tab-content">
                                    <div class="tab-pane active" id="account" aria-labelledby="account-tab" role="tabpanel">

                                        <div class="row">
                                            <div class="col-md-12 table-responsive">
                                                <div class="row">
                                                    <div class="col-lg-3">
                                                        Login Id
                                                        @Html.TextBoxFor(m => m.LoginId, new { @class = "form-control" })
                                                    </div>
                                                    <div class="col-lg-3">
                                                        Name
                                                        @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
                                                    </div>
                                                  
                                                    <div class="col-lg-3">
                                                        <br />
                                                        <input class="btn btn-success" type="submit" value="Search" name="Search" id="btnSearch" />
                                                    </div>
                                                </div>
                                                <hr />
                                               
                                                <div class="dt-responsive table-responsive mt-4">
                                                    <table class="table mb-0" id="example23">

                                                        <thead>
                                                            <tr>
                                                                <th>Sr No.</th>
                                                                <th>Name</th>
                                                                <th>Login Id</th>
                                                                <th>User Type</th>
                                                                <th>Login Date Time</th>
                                                                <th>IP</th>
                                                                <th>Remarks</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @if (Model != null)
                                                            {
                                                                if (Model.lstAssociateLoginLog != null)
                                                                {
                                                                    int i = 1;
                                                                    foreach (var Item in Model.lstAssociateLoginLog)
                                                                    {
                                                                        <tr class="text-left">
                                                                            <td>@i</td>
                                                                            <td>@Item.DisplayName</td>
                                                                            <td>@Item.LoginId</td>
                                                                            <td>@Item.UserType</td>
                                                                            <td>@Item.LoginDateTime</td>
                                                                            <td>@Item.IP</td>
                                                                            <td>@Item.Remarks</td>

                                                                           
                                                                        </tr>
                                                                        i++;
                                                                    }

                                                                }
                                                            }
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                </section>
            </div>
        </div>
    </div>


                        <!-- main-panel ends -->


}
<style>
    .errortext {
        border: 2px solid red !important;
    }
</style>


<script src="../../WebsiteCss/js/jquery.js"></script>
<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.0/themes/smoothness/jquery-ui.css">

<script>

    $('#FromDate').datepicker({
        dateFormat: 'dd/mm/yy',
    });
    $('#ToDate').datepicker({
        dateFormat: 'dd/mm/yy',
    });
</script>
<script>
    $(document).ready(function () {
        $('#dataTables-example').DataTable({
            responsive: true
        });
    });
    $('#printingdate').datepicker({
        dateFormat: 'dd/mm/yy',
        changeYear: true,
        changeMonth: true,

    });
    function changeprintdate(PrintingDate, LoginId) {
        $('#printingdate').val(PrintingDate);
        $('#hdfloginnid').val(LoginId);
        $('#myModal').show();
    }
    function closepopup() {
        $('#myModal').hide();
    }
    function printrecipt() {
        $(".errortext").removeClass("errortext");
        if ($('#printingdate').val() == '') {
            $("#printingdate").addClass('errortext');
            $('#printingdate').focus();
            return false;
        }
        var LoginId = $('#hdfloginnid').val();
        $.ajax({
            url: '/AdminReports/PrintReciept1',
            type: "GET",
            dataType: "JSON",
            data: { id: LoginId, PrintingDate: $('#printingdate').val() },
            success: function (data) {
                debugger;
                window.location.href = '@Url.Action("PrintReciept", "AdminReports")';
            }
        });
    }
</script>





