@model HMGreenCityMLM.Models.Reports

@{
    ViewBag.Title = "TDSReportByLoginID";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



@using (Html.BeginForm("TDSReportByLoginID", "AdminReports", FormMethod.Post, new { @id = "form1" }))
{
    <div class="app-content content">
        <div class="content-wrapper">
          
            <div class="content-body">
                <!-- users edit start -->
                    <div class="card">
                        <div class="card-content">
                            <div class="card-body">
                                <ul class="nav nav-tabs mb-3" role="tablist">
                                    <li class="nav-item">
                                        <a class="nav-link d-flex align-items-center active" id="account-tab" data-toggle="tab" href="#account"
                                           aria-controls="account" role="tab" aria-selected="true">
                                            <i class="feather icon-user mr-25"></i><span class="d-none d-sm-block">TDS Report</span>
                                        </a>
                                    </li>

                                </ul>
                                <div class="tab-content">
                                    <div class="tab-pane active" id="account" aria-labelledby="account-tab" role="tabpanel">
                                        <div class="row">
                                            <div class="col-md-12 table-responsive">
                                                <div class="row">
                                                    <div class="col-md-3">
                                                        PAN Number
                                                        @Html.TextBoxFor(m => m.PAN, new { @class = "form-control", @placeholder = "PAN Number", @maxlength = "10" })
                                                    </div>
                                                    <div class="col-md-3">
                                                        Login ID
                                                        @Html.TextBoxFor(m => m.LoginId, new { @class = "form-control", @placeholder = "Login ID" })
                                                   @Html.HiddenFor(m => m.ToLoginID)
                                                     </div>
                                                    <div class="col-md-3">
                                                        From Date
                                                        @Html.TextBoxFor(m => m.FromDate, new { @class = "form-control", @placeholder = "From Date" })
                                                    </div>
                                                    <div class="col-md-3">
                                                        To Date
                                                        @Html.TextBoxFor(m => m.ToDate, new { @class = "form-control", @placeholder = "To Date" })
                                                    </div>
                                                    <div class="col-md-3">
                                                        <br />
                                                        <input type="submit" class="btn btn-info" value="Get Details" name="GetDetails"  onclick="return validateLogin();"  />
                                                    </div>
                                                </div>

                                                <br />
                                                <table id="example23" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0" width="100%">
                                                    <thead>
                                                        <tr>
                                                            <th>Sr No.</th>
                                                            <th>Date</th>
                                                            <th>Login ID</th>
                                                            <th >Name</th>
                                                            <th>PAN Number</th>
                                                            <th>TDS</th>
                                                            <th>Paid Amount</th>
                                                            <th>Total</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @if (Model != null)
                                    {
                                        if (Model.lsttopupreport != null)
                                        {
                                            int i = 1;
                                            foreach (var Item in Model.lsttopupreport)
                                            {
                                                        <tr class="text-left">
                                                            <td>@i</td>
                                                            <td>@Item.ClosingDate</td>
                                                            <td>@Item.LoginId</td>
                                                            <td >@Item.Name</td>
                                                            <td>@Item.PAN</td>
                                                            <td>@Item.TDSAmount1</td>
                                                            <td>@Item.Amount1</td>
                                                            <td>@Item.GrossAmount1</td>
                                                        </tr>
                                                i++;
                                            }

                                        }
                                    }

                                                    </tbody>

                                                    <tfoot>
                                                        <tr>
                                                            <th></th>

                                                            <th></th>
                                                            <th></th>
                                                            <th></th>
                                                            <th></th>
                                                            <th style="color:dodgerblue;font-weight:800">Total TDs @ViewBag.TotalTDS</th>
                                                            <th></th>
                                                           

                                                        </tr>
                                                    </tfoot>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- content-wrapper ends -->
                            <!-- partial:../../partials/_footer.html -->
                            <!-- partial -->
                        </div>
                    </div>
                
            </div>
        </div>
    </div>

}
<script src="../vendor/jquery/jquery.min.js"></script>

<script src="../../WebsiteCss/js/jquery.js"></script>
<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.0/themes/smoothness/jquery-ui.css">
<script>
    $(document).ready(function () {
        $("input").attr("autocomplete", "off");

        var cityuse = [];
        $("#loader").css({ 'display': '' });
        $.ajax({
            url: '/AdminReports/GetUserList',
            type: 'Post',
            success: function (response) {
                if (response != null) {


                    $(response).each(function (index, item) {
                        cityuse.push(item.UserName + "(" + item.LoginIDD + ")");
                    });

                    $('#LoginId').autocomplete({
                        source: cityuse,
                        select: function (event, ui) {
                            var str = ui.item.label;
                            var strsplit = str.split("(");

                            $('#ToLoginID').val(strsplit[1].substr(0, strsplit[1].length - 1));
                        }
                    })
                }
            }, error: function () {
                alert('Some Thing wrong!');
                $("#loader").css({ 'display': 'none' });
            }
        });
    });
    $('#FromDate').datepicker({
        dateFormat: 'dd/mm/yy',
    });
    $('#ToDate').datepicker({
        dateFormat: 'dd/mm/yy',
    });
    function validateLogin() {
        var login = $('#LoginId').val();
        if (login == "") {
            $('#ToLoginID').val(null);
        } else {
            var loginid = $('#ToLoginID').val();

        }
    }
</script>
